package com.bank.pojos;

import java.util.ArrayList;

public class User 
{
	
	int id;
	String firstname,lastname,password,email;
	ArrayList<Account> accts;
	
	public User()
	{
		
	}
	
	public User(int id, String firstname, String lastname, String password, String email, ArrayList<Account> accts) {
		super();
		this.id = id;
		this.firstname = firstname;
		this.lastname = lastname;
		this.password = password;
		this.email = email;
		this.accts = accts;
	}
	
	

}
