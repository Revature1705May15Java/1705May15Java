package factorial;

public class factorial {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n = 4, result = 1;
		
		for(int i = 0; i < n; i++){
			System.out.println("result: " + result + " n:" + n + "    i = " + i);
			result *= n--;
		}
		System.out.println(result);

	}

}
