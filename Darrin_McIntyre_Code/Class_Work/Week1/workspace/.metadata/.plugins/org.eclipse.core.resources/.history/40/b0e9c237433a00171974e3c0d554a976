
public class Fibonacci {

	public static void main(String[] args) {
		// Fn = Fn-1 + Fn-2...
		
		int n = 4, result = 0;
		int length = n;
		
		for(int i = 2; i < length; i++){
			result += n-(i-1);
			System.out.println("result:" + result + "   " + (n-1));
		}
		if(n == 0 || n == 1) result = 1;
		System.out.println("Fibonacci Number: " + result);
	}

}
