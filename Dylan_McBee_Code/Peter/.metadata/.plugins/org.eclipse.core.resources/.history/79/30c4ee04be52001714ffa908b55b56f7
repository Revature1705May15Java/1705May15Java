package com.revature.model;

import javax.persistence.*;


@Entity
@Table(name = "USER_T")
public class User {
	
	@Id
	@Column(name= "U_ID")
	private int id;
	
	@Column(name= "U_USERNAME")
	private String userName;
	
	@Column(name= "U_PASSWORD")
	private String password;
	
	@Column(name= "U_ID")
	private byte[] image;
	
	
}
