package com.ex.nested;

import com.ex.nested.A.B;

public class Doggo {

	static int doggos= 1;
	
	static class DoggoCount{
		void message(){
			System.out.println("In my inner class! Doggos present " + doggos);
			System.out.println(this.getClass());
			//System.out.println(x); //Cannot reference x because it is not static.
		}
	}
	public static void main(String[] args) {
		A.B objectOfB = new A.B();
		B objB = new B();
		
		objB.message();
		// in order to create an object of B, we must use [outerclass].[innerclass]
		
		objectOfB.message();
	}
	
}
