package com.ex.p5;

import java.util.ArrayList;

public class Prime {

	public static void main(String[] args) {

		ArrayList<Integer> numbers = new ArrayList<Integer>(100);
		for (int i = 1; i <= 100; i++) {
			numbers.add(i);

		}
		for (int i = 2; i <= numbers.size()/2; i++) {
			if (numbers.indexOf(i) % i != 0) {
				System.out.println(numbers.indexOf(i));
			}

		}
	}

}
