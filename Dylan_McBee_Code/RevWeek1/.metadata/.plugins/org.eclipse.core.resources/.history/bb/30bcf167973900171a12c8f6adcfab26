package com.rev.intro;

abstract class Helper{
	
}
public class HelloWorld extends Helper {
	
	public static void main(String[] args) {
		//public = access modifier
		//static = belongs to the class, not the object
		//void = return type
		// main = method name
		// () = parameters
		// String = data type [] array of strings
		// args - var name
		System.out.println("Hello World!");
		
		int a = 0; // 4bytes -2,147,483,648 to 2/147,483,647
		char b = '0'; // 2bytes 0 to 65536
		short c = 0; // 2bytes -32,768 to 32,767
		long d = 0; // 8bytes -9,223,372,036,854,775,808 to ....
		float e = 0.0f; // 4bytes 3.4e-308 to ...   double check
		byte f = 0; // 1byte -128 to 127
		double g = 0.0;
		boolean h = false; // True or False
		
		
	}

}
