package com.ex.p5;

import java.util.ArrayList;

public class Prime {

	public static void main(String[] args) {
		int m = 0;
		ArrayList<Integer> numbers = new ArrayList<Integer>(100);
		for (int i = 1; i <= 100; i++) {
			numbers.add(i);

		}
		for (int i = 0; i <= numbers.size(); i++) {
			m = numbers.get(i)/2;
			for(int p = 0; p<=m; p++)
			if (numbers.indexOf(p) % i != 0) {
				System.out.println(numbers.indexOf(i));
			}

		}
	}

}
