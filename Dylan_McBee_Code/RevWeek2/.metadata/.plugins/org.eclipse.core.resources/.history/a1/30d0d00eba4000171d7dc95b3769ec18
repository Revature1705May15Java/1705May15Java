package com.ex.pojos;

public class Customer {

		int id;
		String fn, ln, state, email;
		double credit;
		
		public Customer(){
			
		}
		public Customer(String fn, String ln, String state, String email, double credit) {
			super();
			this.fn = fn;
			this.ln = ln;
			this.state = state;
			this.email = email;
			this.credit = credit;
		}
}
