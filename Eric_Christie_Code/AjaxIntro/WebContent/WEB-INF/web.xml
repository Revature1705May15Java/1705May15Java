<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>AjaxIntro</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>AjaxGetText</servlet-name>
    <servlet-class>com.ex.servlets.AjaxGetTextServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AjaxGetText</servlet-name>
    <url-pattern>/ajaxGetText</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AjaxGetJson</servlet-name>
    <servlet-class>com.ex.servlets.AjaxGetJsonServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AjaxGetJson</servlet-name>
    <url-pattern>/ajaxGetJson</url-pattern>
  </servlet-mapping>
</web-app>