<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>AJAXIntro</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>GetText</servlet-name>
    <servlet-class>com.rev.servlets.AJAXGetTextServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetText</servlet-name>
    <url-pattern>/getText</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>GetJson</servlet-name>
    <servlet-class>com.rev.servlets.AjaxGetJsonServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetJson</servlet-name>
    <url-pattern>/getJSON</url-pattern>
  </servlet-mapping>
</web-app>