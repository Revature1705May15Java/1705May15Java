package com.reim.pojos;

public class Request {
	int request_id, requester_id, resolver_id, state_id;
	String note, purpose, opened, closed;
	double amount;
	public Request() {
	}
	public Request(int request_id, int requester_id, int resolver_id, int state_id, String note, String purpose,
			String opened, String closed, double amount) {
		super();
		this.request_id = request_id;
		this.requester_id = requester_id;
		this.resolver_id = resolver_id;
		this.state_id = state_id;
		this.note = note;
		this.purpose = purpose;
		this.opened = opened;
		this.closed = closed;
		this.amount = amount;
	}
	
	
}
