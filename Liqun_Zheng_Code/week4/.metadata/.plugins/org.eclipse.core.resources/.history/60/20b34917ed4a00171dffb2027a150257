var parags = document.getElementsByTagName("p");
//var pString = parags.join(" - ");

var inner;

for(i = 0; i < parags.length; i++){
	inner.push(parags[i].innerHTML);
}

document.getElementById("display").innerHTML = inner[1].innerHTML;


