package com.ex.beans;

import java.io.Serializable;

public class Bear extends Animal implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private int weight;
	public Bear(String name, int weight, Cave cave) {
		super();
		this.weight = weight;
		this.name = name;
		this.cave = cave;
	}
	
	
}
