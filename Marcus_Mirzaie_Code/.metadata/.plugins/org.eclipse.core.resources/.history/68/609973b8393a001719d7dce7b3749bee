package q1q;

public class BubbleSort {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] iarray = {1, 0, 5, 6, 3, 2, 3, 7, 9, 8, 4};
		boolean outOfOrder;
		do{
			outOfOrder = false;
			System.out.print(iarray);
			for(int i = 0;i<iarray.length;i++){
				if((i+1<=iarray.length) && iarray[i+1] > iarray[i]){
					outOfOrder = true;
					int tmp = iarray[i];
					iarray[i]=iarray[i+1];
					iarray[i+1]=tmp;
				}
			}
		}while(outOfOrder);
	}

}
