package com.ex.p1;

public class Fibonacci {
	public static void main(String[] args) {
		
	}
	public static int[] Fibonaccis(int howmany){
		int[] arrayOut=new int[howmany];
		arrayOut[0]=0;
		arrayOut[1]=1;
		for(int i=0;i<howmany;i++){
			arrayOut[i]=arrayOut[i-2]+arrayOut[i-2];
		}
		return arrayOut;
	}
}
