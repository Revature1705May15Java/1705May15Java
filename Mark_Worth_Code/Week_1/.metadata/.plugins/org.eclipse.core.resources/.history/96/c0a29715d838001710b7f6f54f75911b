package rev1;

public class Customer {
	private static int CustomerID=0;
	private int newestCustomerID=2;
	public Customer(){
		CustomerID=newestCustomerID;
	}
	public Customer(int CustomerID){
		
	}
	public static int getID(){
		return CustomerID;
	}
	public static String getID(String checknum){
		return "this works";
	}
	public static void main(String[] args) {
		Customer obj=new Customer(3);
		int idcheck =obj.CustomerID;
		System.out.println("Customer ID is : "+idcheck);
	}
}
