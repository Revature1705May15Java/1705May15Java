package com.randomexercises.nested.fourexamples;

class Outer{
	static class Inner{//static nested class 
		
	}
}

class Animal{
	class Dog{//inner class
		void message(){
			System.out.println("bark");
		}
	}
}



abstract class Person{
	void speak(){
		System.out.println("ordinary methods, nothing to see here");
	}
	
}



public class FourInnerClassTypes {


	public static void main(String[] args) {
		Person j = new Person(){
			@Override
			void speak(){
				System.out.println("taking over the methods");
			}
			
		};
		j.speak();
		
		

	}

}
