package p15;

public class Math implements Maths
{
	@Override
	public int add(int a, int b) 
	{
		return a + b;
	}

	@Override
	public int subtract(int a, int b) 
	{
		return a - b;
	}

	@Override
	public int multiply(int a, int b) 
	{
		return a * b;
	}

	@Override
	public int dividie(int a, int b) 
	{
		return a / b;
	}
	
}
