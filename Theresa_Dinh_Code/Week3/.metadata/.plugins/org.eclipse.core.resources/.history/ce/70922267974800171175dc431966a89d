package pojos;

public class Status 
{
	int statusId; 
	String statusName;
	
	public Status() {
		super();
	}
	
	public Status(int statusId, String statusName) {
		super();
		this.statusId = statusId;
		this.statusName = statusName;
	}
	
	public int getStatusId() {
		return statusId;
	}
	
	public void setStatusId(int statusId) {
		this.statusId = statusId;
	}
	
	public String getStatusName() {
		return statusName;
	} 
	
	
}
