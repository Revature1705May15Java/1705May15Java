package reverse;

public class rev {
	
	public String StringRev(String s){
	
		String temp=s;
		s="";

		for(int i=0;i<5;i++){
			System.out.println(temp);
			System.out.println("hi");
		}
			System.out.println("aa");
			System.out.println(temp);
			s+=s.concat(temp);
		
		
		
		return s;
		
	}

}
