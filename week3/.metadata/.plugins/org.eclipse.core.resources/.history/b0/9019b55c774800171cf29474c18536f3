package fibonacci;

public class fibo {

	public int fibo(int n){
		if(n==0){
			return 0;
		}
		else if(n==1){
			return 1;
		}
		else
			return fibo(n-1)+fibo(n-2);
	}

	public int [] fibList(int x){
		int [] flist = new int[x];
		for(int i=0;i<flist.length-1;i++){
			flist[x]=fibo(x);
		}
		for(int a =0;a<flist.length;a++){
			System.out.println(flist[a]);
		}
		
		return flist;
	}

}
