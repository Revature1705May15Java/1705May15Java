package com.corejava;

public class Q6 {
/*
Write a program to determine if an 
integer is even without using the 
modulus operator (%)
 */
	
	public static boolean Q6m(int input) {
		double input2=input;
		return (Math.pow(-1, input2)==1);
	}
	

public static void main(String[] args) {
	System.out.println(Q6m(5));
}
}