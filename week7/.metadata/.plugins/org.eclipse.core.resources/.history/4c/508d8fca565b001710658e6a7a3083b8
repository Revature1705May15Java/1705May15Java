package com.challenges;

import java.util.ArrayList;
import java.util.Scanner;
class Solution{
   
   public static void main(String []argh)
   {
      Scanner sc = new Scanner(System.in);
      Character[] arrL={'(','[','{'};
      Character[] arrR={')',']','}'};
      
      while (sc.hasNext()) {
         String input=sc.next();
          if(input.length()%2!=0){
              System.out.println(false);
          }else{
              int len=input.length();
              ArrayList<Character> chars=new ArrayList<Character>();
              for(int i=0;i<len;i++){
            	  chars.add(input.charAt(i));
              }
              while(chars.size()>0){
            	  boolean hasRun=false;
            	  int size = chars.size();
            	 for(int i=0;i<size;i++){
	            	  int Lpos=findIndex(chars.get(i),arrL);
	            	  if(Lpos>=0){
		            	  Character findR=arrR[Lpos];
		            	  if(false&&findR.equals(chars.get(i+1))){
		            		  chars.remove(i+1);
		            		  chars.remove(i);
		            		  hasRun=true;
		            		  continue;
		            	  }
	            	  }else{
	            		  break;
	            	  }
	            	  if(i==size-2){
	            		  break;
	            	  }
            	 }
              }
	          if(chars.size()==0){
	        	System.out.println(true);
	          }else{
	        	  System.out.println(false);
	          }
          }
      }
      
   }
   public static int findIndex(Character c,Character[] ca){
	   for(int i=0;i<ca.length;i++){
		   if(ca[i]==c){
			   return i;
		   }
	   }
	   return -1;
   }
   public static int findIndex(Character c,ArrayList<Character> ca){
	   for(int i=ca.size()-1;i>-1;i--){
		   if(ca.get(i)==c){
			   return i;
		   }
	   }
	   return -1;
   }
}
