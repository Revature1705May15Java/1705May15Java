import java.util.*;
class TestingGrounds{
   
   public static void main(String []argh)
   {
      Scanner sc = new Scanner(System.in);
      
      while (sc.hasNext()) {
         String input=sc.next();
         Stack<Character> st = new Stack();
         
         for(int i = 0; i < input.length(); i++){
         	st.push(input.charAt(i));
         }
         for(int i = 0; i < input.length(); i++){
          	char c = (char) st.pop();
          	
          }
         
      }
      
   }
}